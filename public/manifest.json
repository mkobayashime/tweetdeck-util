{
  "manifest_version": 2,
  "name": "Chrome mkobayashime",
  "version": "0.10.0",
  "description": "Made for personal use. Use at your own risk.",
  "icons": {
    "16": "icons/icon_16.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "scripts": ["disableForceDownload.js"],
    "persistent": true
  },
  "permissions": ["<all_urls>", "webRequest", "webRequestBlocking"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["quickTazugane.js"]
    },
    {
      "matches": ["https://tweetdeck.twitter.com/"],
      "run_at": "document_end",
      "js": ["tweetdeck.js"]
    },
    {
      "matches": ["https://anime.dmkt-sp.jp/*"],
      "run_at": "document_end",
      "js": ["dAnime.js"]
    },
    {
      "matches": ["https://manaba.tsukuba.ac.jp/*"],
      "run_at": "document_end",
      "js": ["manaba.js"]
    },
    {
      "matches": ["https://www.tulips.tsukuba.ac.jp/search/*"],
      "run_at": "document_end",
      "js": ["tulips.js"]
    },
    {
      "matches": ["https://twins.tsukuba.ac.jp/*"],
      "run_at": "document_end",
      "js": ["twins.js"]
    },
    {
      "matches": ["https://www.nicovideo.jp/*"],
      "run_at": "document_start",
      "js": ["niconico.js"]
    },
    {
      "matches": ["https://app.getmoneytree.com/*"],
      "run_at": "document_end",
      "js": ["moneytree.js"]
    },
    {
      "matches": ["https://www.kurashiru.com/recipes/*"],
      "run_at": "document_end",
      "js": ["kurashiru.js"]
    },
    {
      "matches": ["https://secure.freee.co.jp/*"],
      "run_at": "document_end",
      "js": ["freee.js"]
    },
    {
      "matches": ["https://app.slack.com/*"],
      "run_at": "document_end",
      "js": ["slack.js"]
    }
  ],
  "web_accessible_resources": ["style/tazugane.css"]
}
